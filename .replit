modules = ["web", "nodejs-20", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Web Server"

[[workflows.workflow]]
name = "Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m http.server 5000 --bind 0.0.0.0"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33415
externalPort = 3003

[[ports]]
localPort = 33417
externalPort = 4200

[[ports]]
localPort = 37089
externalPort = 5000

[[ports]]
localPort = 42105
externalPort = 3002

[[ports]]
localPort = 44195
externalPort = 3000

[[ports]]
localPort = 46715
externalPort = 3001

[deployment]
deploymentTarget = "static"
publicDir = "."
